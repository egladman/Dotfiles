#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable



#Backlight Down
"xbacklight -dec 5"
    m:0x8 + c:72
    Alt + F6 

#Backlight Up
"xbacklight -inc 5"
    m:0x8 + c:73
    Alt + F7 

#Mute
"amixer set Master toggle"
    m:0x8 + c:74
    Alt + F8 

#Decrease Volume
"amixer set Master 5- unmute"
    m:0x8 + c:64
    Alt + Alt_L 

#Increase Volume
"amixer set Master 5+ unmute"
    m:0x8 + c:76
    Alt + F10 

#Delete
"xvkbd -xsendevent -text '\[Delete]'"
    m:0x8 + c:22
    Alt + BackSpace 

#End
"xvkbd -xsendevent -text '\[End]'"
    m:0x8 + c:114
    Alt + Right 

#Home
"xvkbd -xsendevent -text '\[Home]'"
    m:0x8 + c:113
    Alt + Left 

#Page Down
"xvkbd -xsendevent -text '\[Page_Down]'"
    m:0x8 + c:116
    Alt + Down 

#Page Up
"xvkbd -xsendevent -text '\[Page_Up]'"
    m:0x8 + c:111
    Alt + Up 

#Dmenu Shortcut
"dmenu_run -h 20 -nb black -p command"
    m:0x40 + c:38
    Mod4 + a 

#cat /proc/sys/kernel/{ostype,osrelease} | sed -n '2p'

#network manager
"wicd-client"
    m:0x40 + c:25
    Mod4 + w 

#Logout Prompt
"oblogout"
    m:0x40 + c:53
    Mod4 + x 

#Filebot
"env LANG=en_US.UTF-8 filebot"
    m:0x40 + c:46
    Mod4 + l 

#oblogout prompt
"oblogout"
    m:0x4 + c:124
    Control + XF86PowerOff 

#
# End of xbindkeys configuration
